digraph "perl6-type-graph" {
    rankdir=BT;
    splines=polyline;
    overlap=false; 
    // Types
    "X::Syntax" [color="#6666FF", fontcolor="#6666FF", href="/type/X::Syntax", fontname="FreeSans"];

